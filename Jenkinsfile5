pipeline{
  agent any
  tools {
      maven 'maven3'
  }
  stages{
      stage('checking'){
          steps{
            sh "mvn clean package"

          }
      }
      stage('build') {
          steps{
            withSonarQubeEnv('sonarqube') {
                sh "mvn sonar:sonar"
            }

          }
      }
  }

}
